{
  "name": "broker_view_generative_ui",
  "yaml": "api.yaml",
  "app": {
    "import_prefix": "",
    "connexion": {
      "enable": false,
      "port": 8191,
      "base_path": "/api",
      "sdk_models": false,
      "sdk_models_dest": "app/sdk_models"
    },
    "fastapi": {
      "enable": true
    },
    "dev_container": {
      "enabled": false,
      "with_dapr": false
    },
    "celery": {
      "enable": false,
      "prefetch_configuration": false
    },
    "db": {
      "enable": false,
      "driver": "postgres",
      "alembic": false,
      "openalchemy": {
        "enable": false,
        "path": "app/models/model_autogenerated.py",
        "module": "app.models.model_autogenerated"
      },
      "sqlalchemy": false
    }
  },
  "client": {
    "version": "1.0.0",
    "python": {
      "enable": true,
      "path": "client",
      "openapi_sdk_generator": true
    },
    "node": {
      "enable": true,
      "path": "node_client"
    }
  }
}