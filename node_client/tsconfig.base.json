{
    "compilerOptions": {
        "declaration": true,
        "declarationDir": "lib/types",
        "baseUrl": ".",
        "module": "esnext",
        "target": "es6",
        "lib": ["es6", "dom", "es2016", "es2017"],
        "sourceMap": false,
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "strict": false,
        "noImplicitAny": true,
        "removeComments": true,
        "preserveConstEnums": true,
    }
}
